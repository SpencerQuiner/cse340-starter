<div class="management-form management-home">
    <h1><%= title %></h1>
    <%- messages() %>
    <% if (errors) { %>
        <ul class="notice">
        <% errors.array().forEach(error => { %>
        <li><%= error.msg %></li>
        <%  }) %>
        </ul>
        <% } %>
    <div class="management-buttons">
        <div class="management-Link">
            <a href="/inv/add-classification">Add Classification</a>
        </div>

        <div class="management-Link">
            <a href="/inv/add-inventory">Add Inventory Item</a>
        </div>
    </div>

    <div class="form-group">
        <label for="classificationList">Select Classification</label>
        <%- classificationList %>
    </div>

    <table id="inventoryDisplay"></table>

    <noscript>JavaScript must be enabled to use this page.</noscript>
</div>


<script src="../../js/inventory.js"></script>